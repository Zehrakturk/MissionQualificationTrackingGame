    font-size: 56px;
    margin-left: 11px;
    margin-right: 11px;
    margin-top: 11px;
    margin-bottom: 11px;
    transition-duration: 0.35s;
    scale: 1.1 1.1;
    -unity-background-image-tint-color: rgb(132, 145, 188);
}

