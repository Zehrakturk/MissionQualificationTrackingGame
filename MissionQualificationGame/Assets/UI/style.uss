.button-style:hover {
    height: 75px;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    width: 252px;
    background-image: url('project://database/Assets/UI/00.png?fileID=-1522430456&guid=4f462fa00fd02e442addee6cfda1935d&type=3#00_34');
    background-color: rgba(127, 127, 127, 0);
