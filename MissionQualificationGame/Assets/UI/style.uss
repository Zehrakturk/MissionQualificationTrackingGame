    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    width: 252px;
    background-image: url('project://database/Assets/UI/00.png?fileID=-1522430456&guid=4f462fa00fd02e442addee6cfda1935d&type=3#00_34');
    background-color: rgba(188, 188, 188, 0);
    color: rgb(241, 237, 237);
